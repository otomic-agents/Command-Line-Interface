{
  "name": "otmoic-cli",
  "description": "otmoic-cli has functions such as generating account reports, querying quotes, and making exchanges.",
  "version": "0.0.6",
  "author": "bytetrade",
  "bin": {
    "otmoiccli": "./bin/run.js",
    "Command-Line-Interface": "./bin/run.js"
  },
  "bugs": "https://github.com/otmoic/Command-Line-Interface/issues",
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "async-retry": "^1.3.3",
    "bignumber.js": "^9.1.2",
    "cli-table3": "^0.6.5",
    "enquirer": "^2.4.1",
    "level": "^8.0.1",
    "listr2": "^8.2.1",
    "needle": "^3.3.1",
    "otmoic-sdk": "^0.0.95",
    "puppeteer": "^22.10.0",
    "superagent": "^10.1.0"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/async-retry": "^1.4.8",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/needle": "^3.3.0",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "mocha": "^10",
    "oclif": "^4",
    "prettier": "^3.4.1",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/otmoic/Command-Line-Interface",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "oclif": {
    "bin": "otmoiccli",
    "commands": "./dist/commands",
    "dirname": "otmoiccli",
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "otmoic/Command-Line-Interface",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme && cp -r node_modules/bignumber.js dist/",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "format": "prettier . --write"
  },
  "types": "dist/index.d.ts"
}
